<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width initial-scale=1.0">
    <!-- local css file path -->
    <link rel="stylesheet" href="css/index.css" />
    <!-- page title and favicon -->
    <title>Typescript</title>
</head>
<body>

    <!-- log_in form -->
    <form action="/log_in" method="post" class="form" id="form" accept-charset="utf-8">
        <input type="email" name="email" id="email" class="input" placeholder="Your Email ..." required />
        <input type="password" name="password" id="password" class="input" placeholder="password" required >
        <button class="button" id="button" type="submit">submit</button>
    </form>

    <!-- local javascript file path -->
    <script src="js/index.js" type="text/javascript" charset="utf-8"></script>
</body>
</html>